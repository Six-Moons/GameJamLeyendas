		{
			"character": "character-1618709165.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "¡A ver qué mal le hace un buen sape a este terco!"
		},
		{
			"character": "character-1618709187.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "¡Pero yo solo quiero demostrarte cuanto te quiero, hermanita!"
